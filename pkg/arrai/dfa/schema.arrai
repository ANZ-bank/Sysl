# This is just a schema of what the output look likes of graph.arrai
{
    appNameInString: (
        types: {
            typeName: (
                map_of: {typesInString},
                type_ref: typeInString, # this is for aliases usually
                fields: {
                    field: {types} # this is basically map_of for each field
                }
            )
        },
        endpoints: {
            endpointNameInString: (
                returns: {typesInString},
                callers: {app: {endpoints}} # whoever calls this endpoint
            )
        }
    )
}