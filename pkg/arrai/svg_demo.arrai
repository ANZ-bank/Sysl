# Parses, transforms and reserializes an SVG document.

let data = //os.file('out/ssd.svg');

let (:macro, :toSvg, :colored, :byText, ...) = //{./svg};
let model = //{./util}.invokeMacro(macro, data);


'<html>' ++
toSvg(
    colored(
        model,
        'green',
        byText(model, 'Read')
    )
)
++ '<style></style></html>'
