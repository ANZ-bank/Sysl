let [_, importSql, sqlSchema, syslPackage] = //os.args;
let spanner = //{./spanner.arrai};
let sysl = //{./sysl.arrai};
let stmts = spanner.parseSchema([importSql]);
let model = spanner.applyStmt(stmts, spanner.emptyModel);
$`${sysl.transformModel(
    cond {
        (model.schema): model,
        _: (
            entities: model.entities,
            indexes: model.indexes,
            schema: {(name: sqlSchema)},
        )
    }, syslPackage)}`
