import deps
import github.com/anz-bank/sysl/tests/bananatree
import github.com/anz-bank/sysl/tests/orchard/lemontree

ModelWithDeps [package="model"]:
  !type Response:
    userId <: int
    id <: int
    title <: string

  /responses:
    /{id<:int}:
      GET:
        Dep <- GET /dep/{id}
        Dep <- GET /moredep/{id}
        Dep2 <- GET /dep2/{id}
        Banana <- GET /banana/{id}
        Lemon <- GET /morelemon
        return Response
