TransformApp:
  !view filename(app <: sysl.App, depPath <: string) -> string:
    app -> (:
      filename = app.name + ".java"
    )

  !view javaFile(app <: sysl.App, basePath <: string) -> string:
    app -> (:

      package = .attrs.package -> <package> (name1:
        packageName = name1
      )
      comment = {"comment1", "comment2"}

      import = {"import1", depPath} -> <set of import>(name:
        importPath = name
      )

      definition = basePath
    )
